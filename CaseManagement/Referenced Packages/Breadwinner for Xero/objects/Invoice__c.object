    <fields>
        <fullName>Status_Flag__c</fullName>
        <deprecated>false</deprecated>
        <description>Red, Yellow or Green flag for Xero Invoices, representing Overdue, Due, and Paid</description>
        <externalId>false</externalId>
        <formula>IF( OR(Status__c = &apos;VOIDED&apos;,Status__c = &apos;DELETED&apos;),
	IMAGE(&quot;https://cdn.daddyanalytics.com/breadwinner/images/voided_deleted.png&quot;, &quot;Deleted / Voided&quot;),
	IF( OR(Status__c = &apos;DRAFT&apos;,Status__c = &apos;SUBMITTED&apos;),
		IMAGE(&quot;https://cdn.daddyanalytics.com/breadwinner/images/draft_submitted.png&quot;, &quot;Draft / Submitted for Approval&quot; ) ,
	IF( Amount_Overdue__c &gt; 0,
	IMAGE(&quot;/img/samples/flag_red.gif&quot;, &quot;Overdue&quot;) ,
	IF( Amount_Due__c &gt; 0,
		IMAGE(&quot;/img/samples/flag_yellow.gif&quot;, &quot;Due&quot;) ,
		IF( AND(Type__c == &apos;Credit Note (Invoice)&apos;,Amount_Credit__c &gt; 0),
		        IMAGE(&quot;/img/samples/flag_yellow.gif&quot;, &quot;Due&quot;) ,
                        IMAGE(&quot;/img/samples/flag_green.gif&quot;, &quot;Paid&quot;)
		)
	)
	)
	)
)</formula>
        <label>Status Flag</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
